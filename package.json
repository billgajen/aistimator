{
  "name": "estimator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:web": "pnpm --filter @estimator/web dev",
    "dev:worker": "pnpm --filter @estimator/worker dev",
    "build": "pnpm -r build",
    "build:web": "pnpm --filter @estimator/web build",
    "build:worker": "pnpm --filter @estimator/worker build",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck",
    "test": "pnpm --filter @estimator/worker test",
    "test:watch": "pnpm --filter @estimator/worker test:watch",
    "test:coverage": "pnpm --filter @estimator/worker test:coverage",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:reset": "supabase db reset",
    "db:migrate": "supabase migration new",
    "db:push": "supabase db push",
    "db:studio": "open http://127.0.0.1:54323"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "prettier": "^3.3.0",
    "typescript": "^5.5.0",
    "vitest": "^4.0.18"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=20.0.0"
  }
}
