{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next",
      "config": {
        "installCommand": "cd ../.. && pnpm install"
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "apps/web/$1" }
  ]
}
